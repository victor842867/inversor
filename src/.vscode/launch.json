    "version": "0.2.0",
    "configurations": [

        {
            "name": "dbg Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/main.c",
            "internalConsoleOptions": "openOnSessionStart",
            "logging": {
                "moduleLoad": false,
